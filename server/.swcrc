{
  "$schema": "https://json.schemastore.org/swcrc",
  "module": {
    "type": "umd"
  },
  "jsc": {
    "target": "es2021",
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "baseUrl": "./",
    "paths": {
      "~*": ["./src/*"]
    }
  }
}